{{ template "base.tmpl" }}
<body>
<div class="card-grid">
    {{ range .Cities }}
        <div class="card">
            <div class="container-card">
                <div class="title">{{ .Name }}</div>
            </div>
            <img src="{{ .ImageURL }}"
                 alt="Image"
                 class="photo">
        </div>
    {{ end }}
</div>
</body>
</html>